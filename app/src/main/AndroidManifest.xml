<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.musicstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/label"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChilledPlaylist" android:label="@string/chilled_label" android:parentActivityName=".MainActivity" />
        <activity android:name=".HoopingPlaylist" android:label="@string/hooping_label" android:parentActivityName=".MainActivity" />
        <activity android:name=".FavoritesPlaylist" android:label="@string/favorites_label" android:parentActivityName=".MainActivity" />
        <activity android:name=".GymPlaylist" android:label="@string/gym_label" android:parentActivityName=".MainActivity" />
    </application>

</manifest>